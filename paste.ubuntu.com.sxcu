{
  "Version": "13.5.0",
  "Name": "Ubuntu Pastebin",
  "DestinationType": "TextUploader",
  "RequestMethod": "POST",
  "RequestURL": "https://paste.ubuntu.com/",
  "Body": "MultipartFormData",
  "Arguments": {
    "poster": "ShareX",
    "syntax": "$prompt:Choose syntax highlighting language|text$",
    "expiration": null,
    "content": "$input$"
  },
  "URL": "$responseurl$"
}
